    <TargetFramework>net8.0</TargetFramework>
    <AssemblyName>Suave.IO</AssemblyName>
    <OutputType>Exe</OutputType>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <None Include="Dockerfile" />
